JAVA_HOME=/opt/java7
PATH=$JAVA_HOME/bin:$PATH
export PATH
UFM_COUNT=`ps -ef |grep ${LOGNAME} | grep -v grep | grep "/opt/mqm/Capitalware/UFM/workflow/CBS182_CEDGERRB_DONE_WORKFLOW.xml"|wc -l`
/usr/bin/date
if [ ${UFM_COUNT} -gt 0 ]
then
        echo "UFM 182 CEDGERRB DONE is already Running.........."
else
	echo "Starting UFM 182 CEDGERRB DONE"
	nohup /opt/mqm/Capitalware/UFM/UFM.sh /opt/mqm/Capitalware/UFM/workflow/CBS182_CEDGERRB_DONE_WORKFLOW.xml > /dev/null &
	echo "Started UFM 182 CEDGERRB DONE"
fi
