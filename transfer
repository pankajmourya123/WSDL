keytool -importcert -file reqres_cert.pem -keystore truststore.jks -alias reqres -storepass changeit
mqsichangeproperties YOUR_NODE_NAME -o BrokerRegistry -n brokerKeystoreFile,brokerKeystoreType,brokerKeystorePass -v /path/to/truststore.jks,JKS,changeit
mqsisetdbparms YOUR_NODE_NAME -n ssl::MySSLProfile -k /path/to/truststore.jks -p changeit
mqsistop YOUR_NODE_NAME
mqsistart YOUR_NODE_NAME
SslProfiles:
  MySSLProfile:
    certificateStore: "/absolute/path/to/truststore.jks"
    certificateStorePassword: "changeit"
mqsistop YOUR_NODE_NAME
mqsistart YOUR_NODE_NAME
